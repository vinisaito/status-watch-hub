# fallback: se level não vier, atualiza level ativo
if level:
    update[f"level{level}_status"] = status
else:
    # pegar nível ativo do timer no estado atual
    level_ativo = get_active_level(chamado)  # função que retorna level1, level2, etc
    if level_ativo:
        update[f"level{level_ativo}_status"] = status