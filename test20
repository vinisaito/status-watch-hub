CREATE EXTERNAL TABLE IF NOT EXISTS sdm_cr_geral_mcs (
  num_chamado STRING,
  equipe STRING,
  gerente_equipe STRING,
  dpto_equipe STRING,
  coord_dpto_equipe STRING,
  ger_dpto_equipe STRING,
  dpto_ger_equipe STRING,
  severidade STRING,
  dat_abertura TIMESTAMP,
  dat_status_concluido TIMESTAMP,
  dat_fechamento TIMESTAMP,
  data_normalizacao TIMESTAMP,
  status STRING,
  classificacao STRING,
  dat_estouro_sla TIMESTAMP,
  num_duracao DOUBLE,
  duracao_sla STRING,
  titulo STRING,
  sla_violado BOOLEAN
)
PARTITIONED BY (year STRING, month STRING)
STORED AS PARQUET
LOCATION 's3://seu-bucket-alerts/parquet/';
