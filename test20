const levelStatusKey = `level${currentLevel}_status`;
const [finalizado, setFinalizado] = useState(
  chamadoData?.statusFinal === "finished" || chamadoData?.[levelStatusKey] === "finished"
);

React.useEffect(() => {
  setFinalizado(
    chamadoData?.statusFinal === "finished" || chamadoData?.[levelStatusKey] === "finished"
  );
}, [chamadoData, levelStatusKey]);