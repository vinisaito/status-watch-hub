import boto3
import json

s3 = boto3.client("s3")

def lambda_handler(event, context):
    obj = s3.get_object(
        Bucket="painel-monitoracao-administrativo-dados",
        Key="api_cache/dados.json"
    )
    data = json.loads(obj["Body"].read())

    return {
        "statusCode": 200,
        "body": json.dumps(data)
    }
