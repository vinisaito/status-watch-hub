SELECT [rdm]
      ,[solicitante]
      ,[requisitante]
      ,[status]
      ,[area_afetada]
      ,[ambiente]
      ,[resumo]
      ,[descricao]
      ,[grupo_executor]
      ,[executor]
      ,[depto_grupo_executor]
      ,[depto_executor]
      ,[recurso_principal]
      ,[data_inicio_programacao]
      ,[data_fim_programacao]
  FROM [mdb].[dbo].[view_ciops_rdm]
